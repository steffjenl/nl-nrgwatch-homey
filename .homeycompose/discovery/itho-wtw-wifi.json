{
  "type": "mdns-sd",
  "mdns-sd": {
    "name": "http",
    "protocol": "tcp"
  },
  "id": "{{host}}",
  "conditions": [
    [
      {
        "field": "host",
        "match": {
          "type": "regex",
          "value": "nrg-itho-[a-z0-9][a-z0-9][a-z0-9][a-z0-9].local"
        }
      }
    ]
  ]
}
